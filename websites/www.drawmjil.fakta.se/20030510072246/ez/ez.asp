<script>
	document.all.htmlbody.focus();
	function insert(what) {
		strSmiley = '<img src="/smileys/'+what+'">';
		oRange=document.selection.createRange();
		oRange.pasteHTML(strSmiley);

	}
	function doFormat(what) {
		if (document.selection.type == 'Text')	{
			document.execCommand(what, arguments[1], arguments[2]);
		}
		else {
			if ('Bold' == what || 'Italic' == what || 'Underline' == what || 'FontSize' == what || 'FontName' == what || 'ForeColor' == what) {
				alert("No text selected");
			}
			else {
				document.execCommand(what, arguments[1], arguments[2]);
			}
		}
	}
