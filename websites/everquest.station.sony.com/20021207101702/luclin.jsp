function placeLayer() {
	contentLayerVar.left = (contentPoint)
	contentLayerVar.top = (contentTop)
	flashLayerVar.left = (flashPoint)
	mainLayerVar.left = (mainLayerPoint)
	leftLayerVar.left = (leftLayerPoint)
